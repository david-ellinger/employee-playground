<% include partials/head %> 
<% include partials/navbar %> 
<div class="container">
    <h2>Employees</h2>
    <table class="table">
        <thead class="thead-dark">
            <tr>   
                <th> ID </th>
                <th> First Name </th>
                <th> Last Name </th>
            </tr>
        </thead>
    <tbody>
    <% if(employees.length) {%>
       <% employees.forEach((e) => { %>
        <tr>
            <td> <%= e.emp_no %> </td>
            <td> <%= e.first_name %> </td>
            <td> <%= e.last_name %> </td>
        </tr>
       <%})%>
    <%}%>
    </tbody>
</div>